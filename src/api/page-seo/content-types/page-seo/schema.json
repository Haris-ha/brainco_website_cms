{
  "kind": "collectionType",
  "collectionName": "page_seos",
  "info": {
    "singularName": "page-seo",
    "pluralName": "page-seos",
    "displayName": "Page SEO",
    "description": "Manage multi-language SEO configuration for all website pages"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "pageName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": false
    },
    "pagePath": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": false
    },
    "metaTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "minLength": 10,
      "maxLength": 60,
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "minLength": 50,
      "maxLength": 160,
      "required": true
    },
    "keywords": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "publisher": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "BrainCo",
      "required": false
    },
    "metaRobots": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "index,follow"
    },
    "xRobotsTag": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "index, follow",
      "enum": [
        "index, follow",
        "noindex, nofollow",
        "index, nofollow",
        "noindex, follow"
      ]
    },
    "canonicalURL": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "ogTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 60,
      "required": false
    },
    "ogDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 160,
      "required": false
    },
    "ogImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "ogType": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "website",
      "enum": [
        "website",
        "article",
        "product"
      ]
    },
    "twitterCard": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "summary_large_image",
      "enum": [
        "summary",
        "summary_large_image",
        "app",
        "player"
      ]
    },
    "twitterTitle": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 60,
      "required": false
    },
    "twitterDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 160,
      "required": false
    },
    "twitterImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "structuredData": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    }
  }
}
